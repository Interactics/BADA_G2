<launch>


    <arg name="cmd_vel_topic" default="/cmd_vel"/>
    <arg name="odom_topic" default="/bada/filtered_odom"/>
	                  
<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
	<param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS"/>
	<param name="recovery_behavior_enabled" value="true"/>
    <param name="clearing_rotation_enabled" value="false"/>
    <rosparam file="$(find bada_g2_2dnav)/config/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
    <rosparam file="$(find bada_g2_2dnav)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find bada_g2_2dnav)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find bada_g2_2dnav)/config/global_costmap_params.yaml" command="load" /> 
    <rosparam file="$(find bada_g2_2dnav)/config/move_base_params.yaml" command="load" />
    <rosparam file="$(find bada_g2_2dnav)/config/dwa_local_planner_params.yaml" command="load" />
    <rosparam file="$(find bada_g2_2dnav)/config/global_planner_params.yaml" command="load" />
    <rosparam file="$(find bada_g2_2dnav)/config/navfn_global_planner_params.yaml" command="load" />
    <remap from="cmd_vel" to="$(arg cmd_vel_topic)"/>-->
    <remap from="odom" to="$(arg odom_topic)"/> 
 </node>

</launch> 



